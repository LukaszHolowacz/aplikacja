<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CFCCCC"
    tools:context=".FirstFragment">

    <Button
        android:id="@+id/clear"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="C"
        android:textColor="#DD4848"
        android:textSize="34sp"
        app:backgroundTint="#A6A1A1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.299" />

    <Button
        android:id="@+id/wynik"
        android:layout_width="70dp"
        android:layout_height="168dp"
        android:text="="
        app:backgroundTint="#FFC89191"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.849" />

    <Button
        android:id="@+id/odejmij"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="-"
        app:backgroundTint="#FFC89191"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.124"
        app:layout_constraintStart_toEndOf="@+id/b0"
        app:layout_constraintTop_toBottomOf="@+id/b3"
        app:layout_constraintVertical_bias="0.265" />

    <Button
        android:id="@+id/b0"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="0"
        app:backgroundTint="#706B6B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toEndOf="@+id/dodaj"
        app:layout_constraintTop_toBottomOf="@+id/b2"
        app:layout_constraintVertical_bias="0.265" />

    <Button
        android:id="@+id/dodaj"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="+"
